{
  "extends": "@tsconfig/node20/tsconfig.json",
  "compilerOptions": {
    "esModuleInterop": true,
    "lib": ["ES2023"],
    "noUncheckedIndexedAccess": true,
    "noFallthroughCasesInSwitch": true,
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "types": ["node"],
    "target": "ES2020",
    "resolveJsonModule": true
  }
}
